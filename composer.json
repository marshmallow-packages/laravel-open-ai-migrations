{
    "name": "marshmallow/laravel-open-ai-migrations",
    "description": "Marshmallow - Laravel Migrations using ChatGPT",
    "keywords": [
        "marshmallow",
        "laravel nova",
        "ChatGPT"
    ],
    "homepage": "https://github.com/marshmallow/laravel-open-ai-migrations",
    "license": "MIT",
    "authors": [
        {
            "name": "Stef van Esch",
            "email": "stef@marshmallow.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.3|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\LaravelOpenAiMigrations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marshmallow\\LaravelOpenAiMigrations\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\LaravelOpenAiMigrations\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
